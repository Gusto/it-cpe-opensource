			"days": 5,
			"next": "prerelease",
			"force_install_days": 4
		},
		"prerelease": {
			"days": 10,
			"next": "release",
			"force_install_days": 9
		},
		"release": {
			"days": null,
			"next": null,
			"force_install_days": 45
		}
	},
	"denylist": [],
